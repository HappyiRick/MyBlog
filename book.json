{
    "author": "irick<happyirick@gmail.com>",
    "description": "A GitBook Blog for irick",
    "extension": null,
    "generator": "site",
    "isbn": "",
    "links": {
        "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        },
        "sidebar": {
            "About Me": "https://happyirick.github.io/",
            "irick's Blog": "https://happyirick.github.io/"
        }
    },
    "output": null,
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "-sharing",
        "-lunr", 
        "-search",
        "multipart",
        "pageview-count",
        "donate",
        "disqus",
        "simple-page-toc",
        "page-toc-button",
        "github",
        "advanced-emoji",
        "search-pro",
        "code",
        "copy-code-button",
        "chapter-fold",
        "back-to-top-button",
        "tbfed-pagefooter",
        "popup",
        "hide-element",
        "toggle-chapters",
        "anchors"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "irick's Blog"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "donate": {
            "wechat": "微信收款的二维码URL",
            "alipay": "支付宝收款的二维码URL",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "github": {
            "url": "https://github.com/HappyiRick"
        },
        "code": {
            "copyButtons": true
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy happyirick 2022",
            "modify_label": "Post time:",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        }

    },
    "title": "irick's Blog",
    "variables": {}
}